<%inherit file="base.mko"/>

<script type="text/javascript">
$(document).ready(function() 
    { 
        $("#failureTable").tablesorter(); 
    } 
);
</script>

<table id="failureTable">
<thead>
  <tr>
    <th>Test</th>
    <th>First Fail</th>
    <th>Last Pass</th>
  </tr>
</thead>
<tbody>
  % for testname in buildtests:
  <tr>
    <td>${testname}</td>
    <td>${buildtests[testname]['firstfail']}</td>
    <td>${buildtests[testname]['lastpass']}</td>
  </tr>
  % endfor  
</tbody>
</table>