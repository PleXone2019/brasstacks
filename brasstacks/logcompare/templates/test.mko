<%inherit file="base.mko"/>



<script type="text/javascript">
$(document).ready(function() 
    { 
        $("#testTable").tablesorter(); 
    } 
);
</script>



<h2>Last 100 results for "${testname}" in ${testtype} for ${product} on ${os}</h2>



<table id="testTable" class="results">
  <thead>
  <tr>
    <th>Run</th>
    <th>Time</th>
    <th>Fail count</th>
    <th>Pass count</th>
    <th>Todo count</th>
  </tr>
  </thead>
  <tbody>
  % for key, value in results:
  <tr>
    <td><a href="/logcompare/run/${value[1]}">See details</a></td>
    <td>${key[3]}</td>
    <td>${value[0][testname]['fail']}</td>
    <td>${value[0][testname]['pass']}</td>
    <td>${value[0][testname]['todo']}</td>
  </tr>
  % endfor
  </tbody>
</table>