<%inherit file="base.mko"/>

<h2>${results[0]['key'][0]}</h2>
<div class="results">
% for (key, value) in results:
<%
  product = value[0]
  platform = value[1]
  testtype = value[2]
  timestamp = value[3]
  result = value[4]
%>
 ${product} ${platform} ${testtype} ${timestamp} ${result['fail']} ${result['pass']} ${result['todo']} <br />
% endfor
</div>